<%-
@pools.each do | pool |
  @vo_name=pool['vo']
  @pool_group=pool['group']
-%>
<% if pool.has_key?('role_voms') -%>
"/<%=@vo_name%>/Role=<%=pool['role_voms']%>/Capability=NULL" <%=@pool_group%>
"/<%=@vo_name%>/Role=<%=pool['role_voms']%>" <%=@pool_group%>
<% else -%>
"/<%=@vo_name%>/Role=NULL/Capability=NULL" <%=@pool_group%>
"/<%=@vo_name%>" <%=@pool_group%>
<% end -%>
<%-
end
-%>
